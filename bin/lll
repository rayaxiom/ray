#!/bin/bash

#/home/ray/learning/phd/wulfling/oomphlib_current/user_drivers/lagrange_square/run_sq0.sh

#PATH TO OOMPH DIR
MYOOMPH_DIR=""
WULFOOMPH_DIR=""
OOMPHPATH=""


# This function does stuff...
towulf()
{
  FILE8=$1
  FromPATH="$MYOOMPH_DIR/$OOMPHPATH"
  ToPATH="$WULFOOMPH_DIR/$OOMPHPATH"

  scpfw $ToPATH/$FILE8 $FromPATH/$FILE8.raytemp > /dev/null
  
  if diff $FromPATH/$FILE8 $FromPATH/$FILE8.raytemp > /dev/null
  then
    echo "$FILE8 is unchanged." > /dev/null
  else
    scptw $FromPATH/$FILE8 $ToPATH/
  fi

  rm -rf $FromPATH/$FILE8.raytemp
}
